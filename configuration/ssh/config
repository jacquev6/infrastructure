# Specific hosts
Host doorman
    HostName 192.168.1.101

Host cloud
    HostName 15.188.248.48

Host v-doorman
    HostName 192.168.1.102

# For subnet behind doorman: Use https://www.redhat.com/sysadmin/ssh-proxy-bastion-proxyjump
# Host toto
#     HostName 10.20.30.40
#     ProxyJump doorman

Host v-node-1
    HostName 10.20.30.50
    ProxyJump ansible@v-doorman

# Defaults
Host *
    HashKnownHosts no
